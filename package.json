{
    "name": "@deepeshgupta/noddy",
    "version": "0.6.0",
    "description": "noddy framework for backend api",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "clean": "rimraf dist/*",
        "lint": "eslint ./src --ext .ts",
        "tsc": "tsc",
        "build": "npm-run-all clean lint tsc",
        "start": "node .",
        "start:dev": "npm-run-all build start",
        "dev": "nodemon --watch src -e ts,mts --exec npm run start:dev",
        "test": "jest"
    },
    "keywords": [
        "noddy"
    ],
    "author": "Deepesh Gupta",
    "license": "MIT",
    "engines": {
        "node": ">=15.x.x <=16.x.x"
    },
    "devDependencies": {
        "@types/cookie-parser": "^1.4.3",
        "@types/cors": "^2.8.12",
        "@types/debug": "^4.1.7",
        "@types/express": "^4.17.13",
        "@types/http-errors": "^1.8.2",
        "@types/lodash": "^4.14.182",
        "@types/node": "^17.0.36",
        "@types/nodemailer": "^6.4.4",
        "@typescript-eslint/eslint-plugin": "^5.27.0",
        "@typescript-eslint/parser": "^5.27.0",
        "eslint": "^8.16.0",
        "npm-run-all": "^4.1.5",
        "rimraf": "^3.0.2",
        "typescript": "^4.7.2"
    },
    "dependencies": {
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "debug": "^4.3.4",
        "dotenv": "^16.0.1",
        "eslint-plugin-neverthrow": "^1.1.4",
        "express": "^4.18.1",
        "http-errors": "^2.0.0",
        "lodash": "^4.17.21",
        "mongoose": "^6.3.5",
        "neverthrow": "^4.3.1",
        "nodemailer": "^6.7.5"
    }
}